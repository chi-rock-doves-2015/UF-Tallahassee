<div class='form'>
  <h2>Log In: </h2>
  <%= form_for(:session, url: login_path) do |f| %>
    <table>
      <tr>
        <td><%= f.label :email %></td>
        <td><%= f.text_field :email %></td>
      </tr>
      <tr>
        <td><%= f.label :password %></td>
        <td><%= f.password_field :password %></td>
      </tr>
      <tr>
        <td> <%= f.submit "Log In" %> </td>
      </tr>
    </table>
  <% end %>
</div>

